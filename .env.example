# ======== Server Config ========
PORT=3001

# ======== Frontend Base URLs ========
FRONT_END_URL=http://localhost:3000
APP_BASE_URL=http://localhost:3000

# ======== MongoDB Atlas ========
MONGO_URI=mongodb+srv://your_username:your_password@your_cluster.mongodb.net/?retryWrites=true&w=majority

# ======== Supabase Auth & DB ========
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# ======== JWT ========
JWT_SECRET=your_jwt_secret_key_here_make_it_long_and_random

# ======== SMTP - Gmail Config ========
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here
SMTP_NAME=no-reply@example.com

# ======== PayOS Config ========
PAYOS_CLIENT_ID=your_payos_client_id_here
PAYOS_API_KEY=your_payos_api_key_here
PAYOS_CHECKSUM_KEY=your_payos_checksum_key_here
